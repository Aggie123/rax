<!doctype html>
<html>

<head>
  <meta charset="utf-8">
  <meta id="viewport" name="viewport"  content="width=device-width" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <meta name="apple-mobile-web-app-status-bar-style" content="black">
  <meta name="format-detection" content="telephone=no">
  <title>MiniApp</title>
</head>

<body>
  <%- externalApi %>

  <% if (isDebug) { %>
  <script src="https://g.alicdn.com/code/npm/??atag/<%= atagVersion %>/dist/atag.js"></script>
  <script src="<%= debugFrameworkURL %>?random=<%= Math.random() %>"></script>
  <% } else { %>
  <script src="https://g.alicdn.com/code/npm/??atag/<%= atagVersion %>/dist/atag.js,miniapp-framework/<%= frameworkVersion %>/dist/web/master.js"></script>
  <% } %>
  <script>
    new MiniApp(<%- appConfig %>);
  </script>

</body>

</html>
